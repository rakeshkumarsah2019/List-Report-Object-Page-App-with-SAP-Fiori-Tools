{
    "welcomeFile": "/app/<%- appName %>/manifest.json",
    "authenticationMethod": "route",
    "routes": [
      {
        "source": "^/app/(.*)$",
        "target": "/$1",
        "service": "html5-apps-repo-rt",
        "authenticationType": "xsuaa"
      },
      {
        "source": "^/sap/(.*)$",
        "target": "/sap/$1",
        "destination": "<%- destination %>",
        "authenticationType": "xsuaa"
      }
    ]
  }
